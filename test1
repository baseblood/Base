PlyrSel.Name = "PlyrSel"
PlyrSel.Parent = Main
PlyrSel.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
PlyrSel.BorderSizePixel = 0
PlyrSel.Position = UDim2.new(0, 110, 0, 462)
PlyrSel.Size = UDim2.new(0, 140, 0, 24)
PlyrSel.Font = Enum.Font.ArialBold
PlyrSel.Text = "Select Player..."
PlyrSel.TextColor3 = Color3.new(1, 1, 1)
PlyrSel.TextSize = 14.000

PlyrSel.MouseButton1Click:connect(function()
	if PlayerList.Size == UDim2.new(0, 125, 0, 0) then
		PlayerList:TweenSize(UDim2.new(0, 125, 0, 130), 'In', 'Linear', 0.2)
	else
		PlayerList:TweenSize(UDim2.new(0, 125, 0, 0), 'In', 'Linear', 0.2)
	end
end)


