_COLKOA.MouseButton1Click:Connect(function()
    if _COLKOAAMOUNT.Text == "0" then 
        _SELECT.Text = "No Bp's"
        wait(1)
        _SELECT.Text = "Selection:"..bpselection
        return 
    end
    bpselection="Koa"
    _SELECT.Text="Selected:"..bpselection
    _SELECT.BackgroundColor3 = BrickColor.new("Rust").Color
end)

