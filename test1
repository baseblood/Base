-- // PLAYER 1 BUTTON \\ --
player1.MouseButton1Click:Connect(function()
	PlyrSel.Text = player1.Text
	PlayerList:TweenSize(UDim2.new(0, 125, 0, 0), 'In', 'Linear', 0.2)
	for i,v in pairs(Workspace.Properties:GetChildren()) do
        if tostring(v.Owner.Value) == PlyrSel.Text then 

            Tag = v.OriginSquare.CFrame

        end
    end
end)

-- // PLAYER 2 BUTTON \\ --
player2.MouseButton1Click:Connect(function()
	PlyrSel.Text = player2.Text
	PlayerList:TweenSize(UDim2.new(0, 125, 0, 0), 'In', 'Linear', 0.2)
	for i,v in pairs(Workspace.Properties:GetChildren()) do
        if tostring(v.Owner.Value) == PlyrSel.Text then 
        
            Tag = v.OriginSquare.CFrame

        end
    end
end)

