                Sbp2(pinedata[i][1],pinedata[i][2],pinedata[i][3])
				wait(0.2)
	            functions.paintb()
			end
			
        elseif tostring(bpselection) == "Palm" then
			    _G.PaintType = "Palm"		
            for i=startbp,endbp do
                Sbp2(palmdata[i][1],palmdata[i][2],palmdata[i][3])
				wait(0.2)
	            functions.paintb()
			end
	
        elseif tostring(bpselection) == "Bamboo" then
			    _G.PaintType = "Bamboo"		
            for i=startbp,endbp do
                Sbp2(bamboodata[i][1],bamboodata[i][2],bamboodata[i][3])
				wait(0.2)
	            functions.paintb()
			end
	
        elseif tostring(bpselection) == "Grey" then
			    _G.PaintType = nill		
            for i=startbp,endbp do
                Sbp2(greydata[i][1],greydata[i][2],greydata[i][3])
				wait(0.2)
	            functions.paintb()
			end

        elseif tostring(bpselection) == "Frost" then
			    _G.PaintType = "Frost"		
            for i=startbp,endbp do
                Sbp2(frostdata[i][1],frostdata[i][2],frostdata[i][3])
				wait(0.2)
	            functions.paintb()
			end	
        end
        startbp=startbp+maxbp
        endbp=endbp+maxbp
        _Build.Text="Build:"..startbp.."-"..endbp
    else
        _Build.Text = "No Colour Selected"
        wait(1)
        _Build.Text = "Build:"..startbp.."-"..endbp
    end

end)

_SKIPLEFT.MouseButton1Click:Connect(function()
    if startbp == 1 then
    else
        startbp=startbp-maxbp
        endbp=endbp-maxbp
        _Build.Text = "Build:"..startbp.."-"..endbp
    end
end)

_SKIPRIGHT.MouseButton1Click:Connect(function()
    startbp = startbp+maxbp
    endbp=endbp+maxbp
    _Build.Text = "Build:"..startbp.."-"..endbp
end)

_DELBPS.MouseButton1Click:Connect(function()
    for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if
            v:FindFirstChild("Owner") and v:FindFirstChild("Type") and
                v.Owner.Value == game.Players.LocalPlayer
         then
            if v.Type.Value == "Blueprint" then
                A_1 = v
                Event = game:GetService("ReplicatedStorage").Interaction.DestroyStructure
                Event:FireServer(A_1)
            end
        end
        end
end)

_BPRESTORE.MouseButton1Click:Connect(function()
        ColorPicker = Instance.new("ScreenGui")

        tool = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
        tool.RequiresHandle = false
        tool.Name = "Blueprint Restore"
        tool.ToolTip = "Changes a Wall into a Blueprint"
        tool.Equipped:connect(
            function(Mouse)
                ColorPicker.Enabled = false
                Mouse.Button1Down:connect(
                    function()
                        if Mouse.Target.Parent:FindFirstChild("Type") then
                            local Cframe
                            if Mouse.Target.Parent:FindFirstChild("MainCFrame") then
                                Cframe = Mouse.Target.Parent.MainCFrame.Value
                            else
                                Cframe = Mouse.Target.Parent.PrimaryPart.CFrame
                            end
                            if Mouse.Target.Parent ~= nil then
                                game.ReplicatedStorage.PlaceStructure.ClientPlacedBlueprint:FireServer(
                                    Mouse.Target.Parent.ItemName.Value,
                                    Cframe,
                                    game.Players.LocalPlayer
                                )
                                game:GetService("ReplicatedStorage").Interaction.DestroyStructure:FireServer(
                                    Mouse.Target.Parent
                                )
                            end
                        else
                        end
                    end)
            end)
    end)


_EXIT.MouseButton1Click:Connect(function()

	Tag=nil
	_G.Text=nil
	ScreenGui:Destroy()
	Main:Destroy()
	Frame:Destroy()
end)

function Sbp(A_1, A_2, A_3)

        data[counting] = {A_1,A_2,A_3}

        counting=counting+1 
end

function Sbp2(A_1, A_2, A_3) 

    game.ReplicatedStorage.PlaceStructure.ClientPlacedBlueprint:FireServer(A_1, A_2, tostring(PlyrSel.Text))
    if (math.random(1,20) == 1) then
        game:GetService("RunService").RenderStepped:wait()
    end
end




starto=1
endo=1

function SetBp(col,num)
    
    endo=starto+num-1
    countthecols=1
    if col == "Koa" then
        for i=starto,endo do
            koadata[countthecols]={data[i][1],data[i][2],data[i][3]}
            koadata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "Spooky" then
        for i=starto,endo do
            spookydata[countthecols]={data[i][1],data[i][2],data[i][3]}
            spookydata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "GoldSwampy" then
        for i=starto,endo do
            golddata[countthecols]={data[i][1],data[i][2],data[i][3]}
            golddata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "Generic" then
        for i=starto,endo do
            genericdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            genericdata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "SnowGlow" then
        for i=starto,endo do
            snowglowdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            snowglowdata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "Oak" then
        for i=starto,endo do
            oakdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            oakdata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "Cherry" then
        for i=starto,endo do
            cherrydata[countthecols]={data[i][1],data[i][2],data[i][3]}
            cherrydata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "LoneCave" then
        for i=starto,endo do
            lonecavedata[countthecols]={data[i][1],data[i][2],data[i][3]}
            lonecavedata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "SpookyNeon" then
        for i=starto,endo do
            spookyneondata[countthecols]={data[i][1],data[i][2],data[i][3]}
            spookyneondata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "Frost" then
        for i=starto,endo do
            frostdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            frostdata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "CaveCrawler" then
        for i=starto,endo do
            cavecrawlerdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            cavecrawlerdata[countthecols][4]=col
            countthecols=countthecols+1
        
        end
    elseif col == "Walnut" then
        for i=starto,endo do
            walnutdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            walnutdata[countthecols][4]=col
            countthecols=countthecols+1
         
        end
    elseif col == "Birch" then
        for i=starto,endo do
            birchdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            birchdata[countthecols][4]=col
            countthecols=countthecols+1
            
        end

    elseif col == "Fir" then
        for i=starto,endo do
            firdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            firdata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "Pine" then
        for i=starto,endo do
            pinedata[countthecols]={data[i][1],data[i][2],data[i][3]}
            pinedata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "Palm" then
        for i=starto,endo do
            palmdata[countthecols]={data[i][1],data[i][2],data[i][3]}
            palmdata[countthecols][4]=col
            countthecols=countthecols+1
            
        end
    elseif col == "Volcano" then
        
        for i=starto,endo do
            volcanodata[countthecols]={data[i][1],data[i][2],data[i][3]}
            volcanodata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "GreenSwampy" then
       
        for i=starto,endo do
            greenswampydata[countthecols]={data[i][1],data[i][2],data[i][3]}
            greenswampydata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "Grey" then
        for i=starto,endo do
            greydata[countthecols]={data[i][1],data[i][2],data[i][3]}
            greydata[countthecols][4]=col
            countthecols=countthecols+1
           
        end
    elseif col == "Bamboo" then
        for i=starto,endo do
            bamboodata[countthecols]={data[i][1],data[i][2],data[i][3]}
            bamboodata[countthecols][4]=col
            countthecols=countthecols+1
           
        end	
   

		
    end
   
    
    for i=starto,endo  do
        
        data[i][4] = col  
      
    end
    starto=num+starto
   
    colours=colours+1
    table.insert(colournumber,col)
    table.insert(colournumber,num)
    table.insert(colourdata,colournumber)
  
    for colourcount,v in pairs(data) do
        table.insert(data[colourcount],tostring(col))
        colourcount=colourcount+1
    end
    if col then
       
        if col == "Koa" then
            _COLKOAAMOUNT.Text = ""..num
        elseif col == "Spooky" then
            _COLSPOOKYAMOUNT.Text = ""..num
        elseif col == "GoldSwampy" then
            _COLGOLDAMOUNT.Text = ""..num
        elseif col == "Generic" then
            _COLGENERICAMOUNT.Text = ""..num
        elseif col == "SnowGlow" then
            _COLSNOWGLOWAMOUNT.Text = ""..num
        elseif col == "Oak" then
            _COLOAKELMAMOUNT.Text = ""..num
        elseif col == "Cherry" then
            _COLCHERRYAMOUNT.Text = ""..num
        elseif col == "LoneCave" then
            _COLLONECAVEAMOUNT.Text = ""..num
        elseif col == "SpookyNeon" then
            _COLSPOOKYNEONAMOUNT.Text = ""..num
        elseif col == "Frost" then
            _COLFROSTAMOUNT.Text = ""..num
        elseif col == "CaveCrawler" then
            _COLCAVECRAWLERAMOUNT.Text = ""..num
        elseif col == "Walnut" then
            _COLWALNUTAMOUNT.Text = ""..num
        elseif col == "Birch" then
            _COLBIRCHAMOUNT.Text = ""..num
        --[[elseif col == "GenericSpecial" then
            _COLGENERICSPECIALAMOUNT.Text = ""..num--]]
        elseif col == "Fir" then
            _COLFIRAMOUNT.Text = ""..num
        elseif col == "Pine" then
            _COLPINEAMOUNT.Text = ""..num
        elseif col == "Palm" then
            _COLPALMAMOUNT.Text = ""..num
        elseif col == "Volcano" then
            _COLVOLCANOAMOUNT.Text = ""..num
        elseif col == "GreenSwampy" then
            _COLGREENSWAMPYAMOUNT.Text = ""..num
       

	   elseif col == "Grey" then
            _COLGREYAMOUNT.Text = ""..num
 
       elseif col == "Bamboo" then
            _COLBAMBOOAMOUNT.Text = ""..num

						
        end
    end
end



function formatloadstring()
text={}
local split
countertext=1
    for i,v in pairs(_G.Text:split("\n")) do
        if string.match(v,"^wait") then
            split = v:split(")Se")
            split[1]=split[1]..")"
            text[countertext]=tostring(split[1])
            countertext=countertext+1
            split[2]="Se"..split[2]
            split2=split[2]:split(")Sb")
            
            if string.find(split2[1],")")  then
                text[countertext]=tostring(split2[1])
                countertext=countertext+1
            else  
                split2[1]=split2[1]..")"
                text[countertext]=tostring(split2[1])
                countertext=countertext+1
                split2[2]="Sb"..split2[2]
                text[countertext]=tostring(split2[2])
                countertext=countertext+1
            end
        else 
            text[countertext]=v
            countertext=countertext+1
        end
    end
    return text
end

function TexttoData(text)

    local A1
    A2 = CFrame.Angles
    TagAndVecs = CFrame.Angles
    local A3
    local SetA1
    local SetA2

    for i, v in pairs(text) do

        if string.match(v,"^Sbp") then

            A1 = string.match(v , "%'.*%'")
            A1 = string.gsub(A1,"'","")
            text2 = v
            newtext = text2:split("Ang(")
            newtext2 = newtext[2]:split(") +")

            TagAndVecs = (CFrame.Angles(newtext2[1]:match("(.+), (.+), (.+)")))

            TagtimesAng = Tag*TagAndVecs

            text2 = v
            Tnewtext = text2:split("Vec(")
            Tnewtext2 = Tnewtext[2]:split("), Plr")
            Vecs = (Vector3.new(Tnewtext2[1]:match("(.+), (.+), (.+)")))
          
            A3 = Plr
       
            A2=(Tag*TagAndVecs)+Vecs
            
            Sbp(A1,Tag*TagAndVecs+Vecs,A3)
       
        elseif string.match(v,"^SetBp") then 
            
            SetA1 = string.match(v, "\".*\"")
            SetA1 = string.gsub(SetA1,"\"","")
            SetA2 = string.match(v,"%d+")
            SetBp(SetA1,SetA2)
        
        else
            
        end
    end
end

repeat
    wait(3)
until Tag ~= nil


