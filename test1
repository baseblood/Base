player6.MouseButton1Click:Connect(function()
    print("pressed6")
	PlyrSel.Text = player6.Text
	PlayerList:TweenSize(UDim2.new(0, 125, 0, 0), 'In', 'Linear', 0.2)
	for i,v in pairs(Workspace.Properties:GetChildren()) do
        if tostring(v.Owner.Value) == PlyrSel.Text then 
           
            Tag = v.OriginSquare.CFrame

        end
    end
end)

	Player = game.Players.LocalPlayer
player1.TextScaled = true
player2.TextScaled = true
player3.TextScaled = true
player4.TextScaled = true
player5.TextScaled = true
player6.TextScaled = true

buttonss = {
	player1,
	player2,
	player3,
	player4,
	player5,
	player6
}

spawn(function()
	while true do
		player1.Text = ""
		player2.Text = ""
		player3.Text = ""
		player4.Text = ""
		player5.Text = ""
		player6.Text = ""
		for i, v in pairs(game.Players:GetChildren()) do
			buttonss[i].Text = v.Name
			buttonss[i].Visible = true
		end
		wait(0.5)
	end
end)




----------------------------------------------------------
_HIGHLIGHTER.MouseButton1Click:Connect(function()
    if _HIGHLIGHTER.Text == "HIGHLIGHTER OFF" then
        _HIGHLIGHTER.Text = "HIGHLIGHTER ON"
        for i,v in pairs(workspace.PlayerModels:GetChildren()) do
            if v:FindFirstChild("Owner") then

                if tostring(v.Owner.Value)== tostring(Plr) then
                    --print("here")
                    if v:FindFirstChild("Type") then
                        if v.Type.Value == "Blueprint" then
                            if v.BuildDependentWood.Material == Enum.Material.Wood then
                                --print("making neon")
                                v.BuildDependentWood.Size = Vector3.new(v.BuildDependentWood.Size.x-0.1,v.BuildDependentWood.Size.y-0.1,v.BuildDependentWood.Size.z-0.1)
                                v.BuildDependentWood.BrickColor = BrickColor.new("Hot pink")
                                v.BuildDependentWood.Material = Enum.Material.Neon
                                v.BuildDependentWood.Transparency = 0.1
                            else
                             end
                        end
                    end
                end
            end
        end    
    else
        _HIGHLIGHTER.Text = "HIGHLIGHTER OFF"
        for i,v in pairs(workspace.PlayerModels:GetChildren()) do
            if v:FindFirstChild("Owner") then

                if tostring(v.Owner.Value)== tostring(Plr) then
                    --print("here")
                    if v:FindFirstChild("Type") then
                        if v.Type.Value == "Blueprint" then
                            if v.BuildDependentWood.Material == Enum.Material.Wood then
                            else
                                --print("removing neon")
                                v.BuildDependentWood.Size = Vector3.new(v.BuildDependentWood.Size.x+0.1,v.BuildDependentWood.Size.y+0.1,v.BuildDependentWood.Size.z+0.1)
                                v.BuildDependentWood.BrickColor = BrickColor.new("Really black")
                                v.BuildDependentWood.Material = Enum.Material.Wood
                                v.BuildDependentWood.Transparency = 0.5
                             end
                        end
                    end
                end
            end
        end    
    end
end)


--------------------------------------------------------------------------------------------------------------------------------------------------

_WOODSOURCE.MouseButton1Click:Connect(function()
if woodbutton == true then
    woodbutton=false
    woodsource=PlyrSel.Text
    _WOODSOURCE.Text = "Wood="..tostring(woodsource)

else
    woodbutton=true
    woodsource=Plr
    _WOODSOURCE.Text="Wood="..tostring(woodsource)

end
end)







