local woodplayer
local function GetCub(WType)
    Planks = {}
    local whichplayer

    if woodsource == Plr then

        whichplayer = Plr
        if PlyrSel.Text == tostring(Plr) then

            woodplayer = Plr
        else

            woodplayer = Plr
        end
    else

        whichplayer = PlyrSel.Text
        if PlyrSel.Text == tostring(Plr) then
     
        else
      
            woodplayer = PlyrSel.Text
        end

    end
 
    cubecount=0
    for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if                                                                   
            v:FindFirstChild("TreeClass") and tostring(v.Owner.Value) == tostring(woodplayer) and 
                v.TreeClass.Value == WType and
                v.WoodSection.Size.Y < LastS + .3
         then
             cubecount=cubecount+1
            Planks[#Planks + 1] = v.WoodSection
        end
    end

    return Planks
end

function PlankFill(CH,WTV)
local Plr = game:GetService("Players").LocalPlayer
otherplayer = PlyrSel.Text
for i,v in pairs(game.Players:GetChildren()) do
    if tostring(v) == tostring(otherplayer) then
        otherplayer=v
    else
    end
end
local Char = Plr.Character
local tempcount=0
local function GetCub(WType)

    local Planks = {}
    for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if
            v:FindFirstChild("TreeClass") and v.Owner.Value  == game:GetService("Players").LocalPlayer and 
                v.TreeClass.Value == WType and
                v.WoodSection.Size.Y < LastS + .3
         then
             tempcount=tempcount+1

            Planks[#Planks + 1] = v.WoodSection
        end
    end
    return Planks
end


    StartCF = Plr.Character.HumanoidRootPart.CFrame
    local Add = 0
    
    for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if
            v:FindFirstChild("TreeClass") and v.Owner.Value ==game:GetService("Players").LocalPlayer and 
                v.TreeClass.Value == WTV and
                v.WoodSection.Size.Y == CH
         then
            Add = Add + 1
        end
    end
    local Start =
        game.Workspace.PlayerModels.ChildAdded:connect(
        function(v)
            if
                v:WaitForChild("TreeClass", 1) and v.TreeClass.Value == WTV and v:WaitForChild("Owner", 1) and
                     v.Owner.Value == game:GetService("Players").LocalPlayer --otherplayer 
             then
                Add = Add + 1
            end
        end
    )
    local bpcount=0
    local BP = {}
    for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if
            v:FindFirstChild("Owner") and v.Owner.Value == otherplayer and-- game:GetService("Players").LocalPlayer and 
                v:FindFirstChild("Type") and
                v.Type.Value == "Blueprint"
         then
             bpcount=bpcount+1
            BP[#BP + 1] = v
        end
    end
    print("BPs",bpcount)


    Start:Disconnect()

    if LastS == nil then
        LastS = CH
    end

    local Tab = GetCub(WTV)

    local C = 0
    local function GetBlock(Val)
        repeat
            C = C + 1
        until Tab[C] == nil or Tab[C].Size.Y == Val
        return Tab[C]
    end

    local LastC
    
    
    --***********************************************************************************
    
    for i, v in pairs(BP) do
        LastC = GetBlock(CH)
        local BPF = Instance.new("BodyPosition")
        BPF.Position = v:GetModelCFrame().Position
        BPF.MaxForce = Vector3.new(10000, 10000, 10000)
        Char.HumanoidRootPart.CFrame = CFrame.new(LastC.Position + Vector3.new(3, 3, 3))
        for i = 1, 5 do
            game.ReplicatedStorage.Interaction.ClientIsDragging:FireServer(LastC.Parent)
             wait(0.07)
        end
        LastC.CFrame = v:GetModelCFrame()
        BPF.Parent = LastC
        spawn(
            function()
                pcall(
                    function()
                        wait(3)
                        BPF:Destroy()
                    end
                )
            end
        )
    end
    Plr.Character.HumanoidRootPart.CFrame = StartCF
end   


------------------------------------------------------------------------------------------------
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

mouse = game.Players.LocalPlayer:GetMouse() --do we need this?

UserInputService = game:GetService("UserInputService")

gui = Title

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)


_MAXBPS.FocusLost:Connect(function()
    maxbp=_MAXBPS.Text
    _MAXBPS.Text = "Bps:"..maxbp
    startbp=1
    endbp=maxbp
    _Build.Text="Build:"..startbp.."-"..endbp
end)



functions.paintb=function()
PlayerName=PlyrSel.Text
for i,v in next, game:GetService"Workspace".PlayerModels:GetChildren() do
    if v:FindFirstChild("Owner") and v.Owner.Value == game.Players[PlayerName] then
        if v:FindFirstChild"Type" and v.Type.Value == "Blueprint" then
            game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(v.ItemName.Value, v.Main.CFrame, PlayerName, _G.PaintType, v, true)
        end
    end
end
end


_Build.MouseButton1Click:Connect(function()
    if bpselection ~= "" then
        if tostring(bpselection) == "Volcano" then
			        _G.PaintType = "Volcano"
            for i=startbp,endbp do
                Sbp2(volcanodata[i][1],volcanodata[i][2],volcanodata[i][3])
            wait(0.2)
			    functions.paintb()
			end

		elseif tostring(bpselection) == "SnowGlow" then
					    _G.PaintType = "SnowGlow"
            for i=startbp,endbp do
                Sbp2(snowglowdata[i][1],snowglowdata[i][2],snowglowdata[i][3])
            wait(0.2)
	            functions.paintb()
			end

        elseif tostring(bpselection) == "GreenSwampy" then
					    _G.PaintType = "GreenSwampy"
            for i=startbp,endbp do
                Sbp2(greenswampydata[i][1],greenswampydata[i][2],greenswampydata[i][3])
            wait(0.2)
	            functions.paintb()
			end

        elseif tostring(bpselection) == "Koa" then
					    _G.PaintType = "Koa"
            for i=startbp,endbp do
                Sbp2(koadata[i][1],koadata[i][2],koadata[i][3])
            wait(0.2)
	            functions.paintb()
			end

       elseif tostring(bpselection) == "Spooky" then
